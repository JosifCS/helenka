{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.tabSize": 4,
	"editor.insertSpaces": false,

	"explorer.confirmDelete": false,

	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.semi": false,

	"[json]": {
		"editor.formatOnSave": true
	},

	"json.schemas": [
		{
			"fileMatch": ["/dictionaries/*.portu.json"],
			"url": "/.vscode/portuSchema.json"
		}
	],

	"i18n-ally.keystyle": "nested",
	"i18n-ally.dirStructure": "file",
	"i18n-ally.enabledFrameworks": ["next-intl"],
	"i18n-ally.translate.engines": ["google"],
	"i18n-ally.translate.google.apiKey": "AIzaSyB9qzswbiOa4hq3HjpGRc4PoVxmYF2doGo",
	"i18n-ally.localesPaths": "messages",
	"i18n-ally.sourceLanguage": "cs",
	"i18n-ally.editor.preferEditor": true,
	"i18n-ally.tabStyle": "tab",
	"i18n-ally.annotationInPlace": true,
	"i18n-ally.displayLanguage": "cs",
	"i18n-ally.extract.autoDetect": false
}
